@prefix ex: <http://twin-example/zurich#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix config: <http://twin-example/config#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix saref4agri: <https://w3id.org/def/saref4agri>.
@prefix saref: <https://saref.etsi.org/core/> .
@prefix irrig: <http://www.w3id.org/def/irrig#>.
@prefix ssn-system: <http://www.w3.org/ns/ssn/systems/> .
@prefix ssn: <http://www.w3.org/ns/ssn/>.
@prefix om:<http://www.ontology-of-units-of-measure.org/resource/om-2> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix caso: <http://www.w3id.org/def/caso#> .


ex:obs_tempsens_0 rdf:type irrig:TemperatureObservation , sosa:Observation ;
    rdfs:label "Temperature observation";
    sosa:madeBySensor ex:tempsens_0;
    sosa:observedProperty irrig:property_air_temperature;
    sosa:hasFeatureOfInterest irrig:featureOfInterest_air;
    sosa:hasResult [om:hasUnit om:degreeCelsius].

ex:ded_tempsens_0 rdf:type irrig:TemperatureDeduction , caso:Deduction;
    rdfs:label "Temperature deduction";
    prov:wasInformedBy ex:obs_tempsens_0;
    sosa:hasFeatureOfInterest irrig:featureOfInterest_air;
    caso:hasResultState irrig:state_temperature_high.

ex:obs_tempsens_1 rdf:type irrig:TemperatureObservation , sosa:Observation ;
    rdfs:label "Temperature observation";
    sosa:madeBySensor ex:tempsens_1;
    sosa:observedProperty irrig:property_air_temperature;
    sosa:hasFeatureOfInterest irrig:featureOfInterest_air;
    sosa:hasResult [om:hasUnit om:degreeCelsius].

ex:ded_tempsens_1 rdf:type irrig:TemperatureDeduction , caso:Deduction;
    rdfs:label "Temperature deduction";
    prov:wasInformedBy ex:obs_tempsens_1;
    sosa:hasFeatureOfInterest irrig:featureOfInterest_air;
    caso:hasResultState irrig:state_temperature_high.

