    sosa:observedProperty irrig:property_rootZone_moistureLevel;
    caso:hasResultState irrig:state_rootZone_moistureLevel_low.

#SHACL reasoner - tree water need
ex:obs_tree_water_need_0 rdf:type irrig:TreeWaterNeedDeduction , caso:Deduction;
    rdfs:label "Tree water need observation";
    prov:wasInformedBy ex:ded_cap_soil_sens_0 , ex:ded_tree_stage_0, ex:ded_tree_growth_0, ex:ded_weather_api_0;
    sosa:madeBySensor ex:shacl_0;
    sosa:hasFeatureOfInterest ex:tree_52716;
    sosa:observedProperty irrig:property_tree_waterNeed;
    sosa:hasResult [om:hasUnit caso:state].

ex:ded_shacl_0 rdf:type irrig:TreeWaterDeduction , caso:Deduction;
    prov:wasInformedBy ex:obs_tree_water_need_0;
    rdfs:label "Tree water need deduction";
    sosa:hasFeatureOfInterest ex:tree_52716;
    sosa:observedProperty irrig:property_tree_waterNeed;
    caso:hasResultState irrig:state_tree_waterNeed_yes.

# SRF Weather API
ex:obs_weather_api_0 rdf:type irrig:WeatherApiObservation , sosa:Observation;
