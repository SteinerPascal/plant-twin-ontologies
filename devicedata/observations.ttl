    sosa:hasResult [om:hasUnit om:millimeter].

ex:ded_weather_api_0 rdf:type irrig:WeatherApiObservation , caso:Deduction;
    prov:wasInformedBy ex:obs_weather_api_0;
    rdfs:label "Weather rain forecast deduction";
    sosa:hasFeatureOfInterest irrig:featureOfInterest_rain;
    sosa:observedProperty irrig:property_rain_quantity;
    caso:hasResultState irrig:state_rain_quantity_no.
