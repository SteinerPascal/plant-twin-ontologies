#Capacitive soil sensor observation
ex:obs_cap_soil_sens_0 rdf:type irrig:CapacitiveSoilObservation , sosa:Observation;
    sosa:madeBySensor ex:cap_soil_sens_0;
    rdfs:label "Capacitive soil observation";
    sosa:hasFeatureOfInterest ex:tree_52716;
    sosa:observedProperty irrig:property_rootZone_moistureLevel;
    sosa:hasResult [om:hasUnit om:percent]. 

ex:ded_cap_soil_sens_0 rdf:type irrig:CapacitiveSoilDeduction , caso:Deduction;
    rdfs:label "capacitive soil deduction";
    prov:wasInformedBy ex:obs_cap_soil_sens_0;
