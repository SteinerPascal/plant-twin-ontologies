<http://www.w3id.org/def/irrig> rdf:type owl:Ontology ;
                                 owl:versionIRI <http://www.w3id.org/def/irrig/201912> ;
                                 owl:imports <https://raw.githubusercontent.com/Irstea/caso/master/ontology/caso.owl> ;
                                 dc:description """IRRIG is an ontology specified for modeling data from a decision support system (DSS) experimented in UR1471 TSCF - Research Unit Technologies and Information Systems for Agrosystems, INRAE, center Clermont Ferrand. The DSS automatize the IRRINOVÂ©, a manual irrigation method build by Arvalis and their partners. 
The website of TSCF: https://www6.ara.inrae.fr/tscf/"""@en ;
                                 dc:publisher <https://www6.clermont.inrae.fr/tscf> ;
                                 dc:title "Irrigation ontology for experimentation in TSCF"@en ;
                                 terms:contributor <https://orcid.org/0000-0002-0421-452X> ,
                                                   <https://orcid.org/0000-0002-7011-4535> ;
                                 terms:creator <https://orcid.org/0000-0002-3076-5499> ,
                                               <https://orcid.org/0000-0002-3517-0945> ,
                                               <https://orcid.org/0000-0003-3587-0367> ;
                                 terms:license <http://purl.org/NET/rdflicense/cc-by4.0> ;
                                 vann:preferredNamespacePrefix "irrig" ;
                                 vann:preferredNamespaceUri "http://www.w3id.org/def/irrig#"^^xsd:anyURI ;
                                 owl:versionInfo "1.2.0" .

