ex:TempSens_0 rdf:type sosa:Sensor ;
	rdfs:label "Temperature Sensor".

<urn:uuid:cf6464b1-89fe-40e1-a4d2-951fd6759949> config:hostedOn config:localmongo.

config:localmongo rdf:type config:MongoEndpoint;
	config:urlEndpoint "http://localhost:3001/api/td/" ;
	config:databaseName "tddatabase";
	config:collectionName "tdcollection".

ex:TempSens_1 rdf:type sosa:Sensor ;
	rdfs:label "TemperatureSens_193".

ex:Phytl_1 rdf:type sosa:Sensor ;
    sosa:observes ex:Oak_0;
	rdfs:label "PhytlSigns_1";
	ex:hasThingDescription <urn:uuid:cf6464b1-89fe-40e1-a4d2-951fd6759949>.

ex:Phytl_2 rdf:type sosa:Sensor ;
    sosa:observes ex:plat_3;
